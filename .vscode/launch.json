{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Electron App",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
			"windows": {
				"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
			},
			"args": [
				"dist"
			],
			"outputCapture": "std",
			"sourceMaps": true,
			"preLaunchTask": "${defaultBuildTask}"
		}
	]
}